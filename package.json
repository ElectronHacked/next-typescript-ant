{
  "name": "yo-nextjs-typescript-generator",
  "version": "1.0.0",
  "description": "Yoman Generator for yo-nextjs-typescript-generator",
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "lint-staged",
    "changelog:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin",
    "changelog:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin",
    "changelog:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin",
    "release": "git push origin --tags",
    "release-and-publish": "npm run release && npm publish"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,json,css}": [
        "prettier --single-quote --write",
        "git add"
      ]
    },
    "ignore": [
      "_*.js"
    ]
  },
  "keywords": [
    "yeoman-generator",
    "react",
    "create-next-app",
    "redux-saga"
  ],
  "files": [
    "generators"
  ],
  "author": "Electron Hacked (https://github.com/ElectronHacked)",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/redux-actions": "^2.3.1",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "config": "^3.0.1",
    "eslint-plugin-react-hooks": "^1.0.1",
    "husky": "^0.14.3",
    "less": "^3.9.0",
    "lint-staged": "^7.1.3",
    "mini-css-extract-plugin": "^0.5.0",
    "mkdirp": "^0.5.1",
    "next": "^7.0.2",
    "next-compose-plugins": "^2.1.1",
    "next-runtime-dotenv": "^1.0.1",
    "node-sass": "^4.11.0",
    "nprogress": "^0.2.0",
    "prettier": "^1.12.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-i18next": "^10.0.1",
    "redux-actions": "^2.6.4",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.1",
    "yeoman-generator": "^2.0.5"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-config-yeoman": "^0.0.1",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "generate-changelog": "^1.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ElectronHacked/yo-nextjs-typescript-generator"
  }
}
